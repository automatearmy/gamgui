{
  "name": "gamgui-frontend",
  "type": "module",
  "version": "0.1.0",
  "description": "Frontend for GAMGUI - A self-hosted, open-source, centrally managed, auditable GAM server for Google IT Admins",
  "author": "Automate Army <team@automatearmy.com>",
  "license": "MIT",
  "homepage": "https://gamgui.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/automatearmy/gamgui.git"
  },
  "bugs": {
    "url": "https://github.com/automatearmy/gamgui/issues"
  },
  "keywords": [
    "google",
    "workspace",
    "gam",
    "admin",
    "management",
    "react",
    "typescript",
    "vite"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "concurrently --names \"proxy,frontend\" --prefix-colors \"blue,green\" --prefix \"[{name}]\" \"npm run dev:proxy\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:proxy": "tsx src/server/server.ts",
    "build": "npm run build:frontend && npm run build:proxy",
    "build:frontend": "vite build",
    "build:proxy": "esbuild src/server/server.ts --bundle --platform=node --format=esm --packages=external --outfile=dist/server.js",
    "start": "node ./dist/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo 'no tests'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-oauth/google": "^0.12.1",
    "@remixicon/react": "^4.6.0",
    "@tabler/icons-react": "^3.34.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.80.6",
    "@types/js-cookie": "^3.0.6",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "http-proxy-middleware": "^2.0.6",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "nocache": "^4.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.5.0",
    "recharts": "^2.15.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
